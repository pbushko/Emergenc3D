<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.pbush.modelhelp.MainActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/main_menu_text1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" >
    </ImageView>

    <Button
        android:id="@+id/button_load_picture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="loadImagefromGallery"
        android:text="Load Picture"></Button>

    <Button
        android:id="@+id/button_load_GL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="goToGL"
        android:text="Load GL"></Button>

</LinearLayout>
